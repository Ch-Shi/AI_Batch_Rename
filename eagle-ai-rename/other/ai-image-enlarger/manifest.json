{
    "id": "ai-image-enlarger",
    "version": "1.0.9",
    "platform": "all",
    "arch": "all",
    "name": "{{manifest.app.name}}",
    "logo": "./logo.png",
    "keywords": ["enlarge", "scale"],
    "fallbackLanguage": "en",
    "languages": ["en", "zh_TW", "zh_CN", "ja_JP", "es_ES", "ru_RU", "de_DE", "ko_KR"],
    "main": {
        "multiple": false,
        "frame": false,
        "vibrancy": true,
        "url": "index.html",
        "width": 1280,
		"minWidth": 800,
        "height": 720,
		"minHeight": 600,
        "opacity": 0
    }
}
